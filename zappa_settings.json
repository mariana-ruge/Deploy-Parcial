{
  "dev": {
    "app_function": "app.app",
    "project_name": "aws-dolar-zappa",
    "runtime": "python3.11",
    "aws_region": "us-east-1",
    "s3_bucket": "zappa-deploy-xxxx",
    "timeout_seconds": 30,
    "memory_size": 128,

    "manage_roles": false,
    "role_name": "LabRole",
    "role_arn": "arn:aws:iam::468056247368:role/LabRole",

    "apigateway_enabled": false,
    "environment_variables": {
      "BUCKET_NAME": "dolar-raw-dev",
      "S3_BUCKET": "dolar-raw-dev",
      "DB_HOST": "your-rds-endpoint",
      "DB_PORT": "3306",
      "DB_USER": "finanzas_user",
      "DB_PASSWORD": "****",
      "DB_NAME": "finanzas",
      "DB_TABLE": "dolar"
    },
    "events": [
      {
        "function": "app.fetch_and_store",
        "expression": "cron(0 23 * * ? *)"
      },
      {
        "function": "processor.process_s3_event",
        "event_source": {
          "arn": "arn:aws:s3:::dolar-raw-30b4eb32",
          "events": ["s3:ObjectCreated:*"],
          "key_filters": [{ "type": "suffix", "value": ".json" }]
        }
      }
    ],
    "vpc_config": {
      "SubnetIds": ["subnet-0ee4cffed62da7265", "subnet-0bddcfb0793ba78f4"],
      "SecurityGroupIds": ["sg-0d5de39e549148764"]
    }
  }
}
